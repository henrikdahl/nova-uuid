{
    "identifier": "innoract.uuid",
    "name": "UUID",
    "organization": "Innoract",
    "version": "1.0",
    "description": "Provides access to quickly generate a random UUID.",
    "repository": "https://github.com/henrikdahl/nova-uuid",
    "bugs": "https://github.com/henrikdahl/nova-uuid/issues",
    "categories": [
        "commands"
    ],
    "main": "main.dist.js",
    "commands": {
        "editor": [
            {
                "title": "Generate UUID",
                "command": "uuid.generateUUID",
                "shortcut": "cmd-opt-u",
                "when": "editorHasFocus"
            }
        ]
    }
}
